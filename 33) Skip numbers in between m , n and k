import java.util.Scanner;
public class skipnumbers{
    public static void main(String[] args){
        Scanner scanner=new Scanner(System.in);
        int i,M,N,K;
        System.out.print("enter the first number for M:");
        M=scanner.nextInt();
        System.out.print("enter the first number for N:");
        N=scanner.nextInt();
        System.out.print("enter the first number for K:");
        K=scanner.nextInt();
        for(i=M;i<=N;i+=(K+1)){
            System.out.println(i);
    }
    scanner.close();
    }
}
