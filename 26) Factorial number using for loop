import java.util.Scanner;
public class factorial{
    public static void main(String[] args){
        Scanner scanner=new Scanner(System.in);
        int i,n,fact=1;
        System.out.print("enter the number:");
        n=scanner.nextInt();
        for(i=1;i<=n;i++){
            fact=fact*i;
        }
        System.out.println("factorial of " +n+ " is:"+fact);
    }
}
